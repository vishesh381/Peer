<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Owner_Change_When_Complete</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    Status__c = &quot;Complete&quot;,
    ISCHANGED(OwnerId)
)</errorConditionFormula>
    <errorMessage>You cannot change the Owner once the status is marked as Complete.</errorMessage>
</ValidationRule>
